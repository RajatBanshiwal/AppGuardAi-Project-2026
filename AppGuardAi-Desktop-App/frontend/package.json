{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "electron": "cross-env NODE_ENV=development electron ./electron/electron.js",
    "electron:prod": "cross-env NODE_ENV=production electron ./electron/electron.js",
    "build": "vite build",
    "dist": "vite build && electron-builder",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && cross-env NODE_ENV=development electron ./electron/electron.js\""
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.16",
    "aos": "^2.3.4",
    "lucide-react": "^0.552.0",
    "nodemon": "^3.1.10",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.5",
    "tailwindcss": "^4.1.16"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.0.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "vite": "^7.1.7",
    "wait-on": "^9.0.1"
  }
}
