# AppGuard AI Environment Variables

# API Configuration
VITE_API_BASE_URL=http://localhost:3000/api
VITE_APP_NAME=AppGuard AI
VITE_APP_VERSION=1.0.0

# Development Settings
VITE_DEV_MODE=true

# Security Settings
VITE_ENABLE_HTTPS=false

# File Upload Settings
VITE_MAX_FILE_SIZE=100MB
VITE_ALLOWED_FILE_TYPES=.apk,.exe,.dmg,.app

# AI Model Settings
VITE_AI_MODEL_ENDPOINT=http://localhost:5000
VITE_AI_CONFIDENCE_THRESHOLD=0.85

# Logging
VITE_LOG_LEVEL=info
